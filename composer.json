{
  "name": "tourze/php-packer-ast",
  "description": "AST管理和解析工具",
  "type": "library",
  "require": {
    "php": "^8.1",
    "nikic/php-parser": "^5.3.0",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PhpPacker\\Ast\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPacker\\Ast\\Tests\\": "tests/"
    }
  }
}